<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Bikely</title>
    <meta name="description" content="Latest updates and statistic charts">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
    <script>
        WebFont.load({
            google: { "families": ["Poppins:300,400,500,600,700", "Roboto:300,400,500,600,700"] },
            active: function () {
                sessionStorage.fonts = true;
            }
        });
    </script>
    <link href="~/assets/vendors/custom/fullcalendar/fullcalendar.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/vendors/base/vendors.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/demo/default/base/style.bundle.css" rel="stylesheet" type="text/css" />
    <link rel="shortcut icon" href="~/assets/demo/default/media/img/logo/favicon.ico" />
    @Styles.Render("~/Content/css")
</head>
<body class="m-page--fluid m--skin- m-content--skin-light2 m-header--fixed m-header--fixed-mobile">
    <div class="m-grid m-grid--hor m-grid--root m-page">
        <header class="m-grid__item    m-header " data-minimize-offset="200" data-minimize-mobile-offset="200">
            <div class="m-container m-container--fluid m-container--full-height">
                <div class="m-stack m-stack--ver m-stack--desktop">
                    <div class="m-stack__item m-brand  m-brand--skin-dark ">
                        <div class="m-stack m-stack--ver m-stack--general">
                            <div class="m-stack__item m-stack__item--middle m-brand__logo">
                                <a href="" class="m-brand__logo-wrapper">
                                    <img alt="" src="~/assets/demo/default/media/img/logo/rsz_1baku.png" style="margin-bottom: 7px;">
                                </a>
                            </div>
                            <div class="m-stack__item m-stack__item--middle m-brand__tools">
                                <a id="m_aside_header_menu_mobile_toggle" href="javascript:;" class="m-brand__icon m-brand__toggler m--visible-tablet-and-mobile-inline-block">
                                    <span></span>
                                </a>
                                <a id="m_aside_header_topbar_mobile_toggle" href="javascript:;" class="m-brand__icon m--visible-tablet-and-mobile-inline-block">
                                    <i class="flaticon-more"></i>
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="m-stack__item m-stack__item--fluid m-header-head" id="m_header_nav">
                        <div id="m_header_menu" class="m-header-menu m-aside-header-menu-mobile m-aside-header-menu-mobile--offcanvas  m-header-menu--skin-light m-header-menu--submenu-skin-light m-aside-header-menu-mobile--skin-dark m-aside-header-menu-mobile--submenu-skin-dark ">
                            <ul class="m-menu__nav  m-menu__nav--submenu-arrow ">
                                <li class="m-menu__item m-menu__item--submenu m-menu__item--rel m-menu__item--open-dropdown" data-menu-submenu-toggle="click" data-redirect="true" aria-haspopup="true">
                                    @*@Html.ActionLink("Home", "Index", "Home")*@
                                    <a href="@Url.Action("Index","Home")" class="m-menu__link">
                                        @*<i class="m-menu__link-icon flaticon-add"></i>*@
                                        <span class="m-menu__link-text">
                                            Ana səhifə
                                        </span>
                                    </a>
                                </li>
                                <li class="m-menu__item m-menu__item--submenu m-menu__item--rel m-menu__item--open-dropdown" data-menu-submenu-toggle="click" data-redirect="true" aria-haspopup="true">
                                    <a href="@Url.Action("About","Home")" class="m-menu__link">
                                        @*<i class="m-menu__link-icon flaticon-add"></i>*@
                                        <span class="m-menu__link-text">
                                            Haqqında
                                        </span>
                                    </a>
                                </li>
                                <li class="m-menu__item m-menu__item--submenu m-menu__item--rel m-menu__item--open-dropdown" data-menu-submenu-toggle="click" data-redirect="true" aria-haspopup="true">
                                    <a href="@Url.Action("Contact","Home")" class="m-menu__link">
                                        @*<i class="m-menu__link-icon flaticon-add"></i>*@
                                        <span class="m-menu__link-text">
                                            Əlaqə
                                        </span>
                                    </a>
                                </li>
                            </ul>
                        </div>

                        <div id="m_header_topbar" class="m-topbar  m-stack m-stack--ver m-stack--general">
                            <div class="m-stack__item m-topbar__nav-wrapper">
                                <ul class="m-topbar__nav m-nav m-nav--inline">
                                    @Html.Partial("_LoginPartial")
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>

        <div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop m-body">
            <div class="m-grid__item m-grid__item--fluid m-wrapper">
                @RenderBody()
            </div>
        </div>

        <footer class="m-grid__item m-footer ">
            <div class="m-container m-container--fluid m-container--full-height m-page__container">
                <div class="m-stack m-stack--flex-tablet-and-mobile m-stack--ver m-stack--desktop">
                    <div class="m-stack__item m-stack__item--left m-stack__item--middle m-stack__item--last">
                        <span class="m-footer__copyright">
                            @DateTime.Now.Year © Bikely by
                            <span class="main-color">Ayshan Safarova</span>
                        </span>
                    </div>
                </div>
            </div>
        </footer>
    </div>

    <div class="m-scroll-top m-scroll-top--skin-top success" data-toggle="m-scroll-top" data-scroll-offset="500" data-scroll-speed="300">
        <i class="la la-arrow-up"></i>
    </div>
    <script src="~/assets/vendors/base/vendors.bundle.js" type="text/javascript"></script>
    <script src="~/assets/demo/default/base/scripts.bundle.js" type="text/javascript"></script>
    <script src="~/assets/vendors/custom/fullcalendar/fullcalendar.bundle.js" type="text/javascript"></script>
    <script src="~/assets/app/js/dashboard.js" type="text/javascript"></script>
    <script src="~/assets/snippets/pages/user/login.js" type="text/javascript"></script>
    <script src="~/Scripts/site.js"></script>
    @Scripts.Render("~/assets/js")
    @RenderSection("scripts", required: false)
</body>
    </html>
